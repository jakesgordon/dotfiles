[core]
	autocrlf = input
  excludesfile = ~/.gitignore
  pager = delta
[interactive]
  diffFilter = delta --color-only
[delta]
  navigate = true
[merge]
  conflictstyle = diff3
[diff]
  colorMoved = default
[color]
	ui = auto
[push]
	default = simple
[clean]
  requireForce = false
[include]
  path = .gitconfig.local
[init]
	defaultBranch = main
