[core]
	autocrlf = input
  excludesfile = ~/.gitignore
[merge]
  conflictstyle = diff3
[diff]
  colorMoved = default
[color]
	ui = auto
[push]
	default = simple
[clean]
  requireForce = false
[include]
  path = .gitconfig.local
[init]
	defaultBranch = main
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
